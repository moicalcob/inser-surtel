            }
        }
        stage('Test') { 
            steps {
                sh 'cd /home/ubuntu/docker && ls'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'cd /home/ubuntu/docker && ls'
            }
        }
    }
}